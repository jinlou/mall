page {
  background: #f5f5f5;
}

.container {
  position: absolute;
  background-color: #f2f2f2;
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  color: #939393;
}

.nav_right .nav_right_items text {
  display: block;
  margin-top: 5px;
  font-size: 10px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.input_outter {
  height: 44px;
  line-height: 44px;
  background: #e6e6e6;
  position: relative;
  padding: 7px 50px;
  box-sizing: border-box;
}

.input_outter image {
  position: absolute;
  width: 14px;
  height: 14px;
  top: 15px;
  left: 70px;
}

.input_outter input {
  background: #fff;
  border-radius: 30rpx;
  padding: 0 80rpx;
  font-size: 30rpx;
  height: 60rpx;
}

.classification {
  display: flex;
  justify-content: space-between;
  padding: 0 100rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  position: relative;
  box-sizing: border-box;
  background: #fff;
}

.classification image {
  width: 32rpx;
  height: 32rpx;
}

.allClassificate1 {
  position: absolute;
  top: 80rpx;
  left: 0;
  width: 100%;
  padding: 0 100rpx;
  z-index: 2;
  background: #fff;
  display: flex;
  flex-wrap: wrap;
  box-sizing: border-box;
  border-top: 2px solid #e6e6e6;
}

.allClassificateItem {
  display: inline-block;
  width: 33%;
  text-align: center;
}

.goodsItem {
  /* display: flex; */
}

.goodsLeft {
  width: 160rpx;
  height: 200rpx;
  display: inline-block;
  box-sizing: border-box;
  padding: 20rpx 0;
}

.goodsLeft image {
  width: 100%;
  height: 100%;
  /* width: 160rpx;
  height: 160rpx;
  display: inline-block; */
}

.goodsRight {
  display: inline-block;
  flex-direction: column;
  height: 200rpx;
  width: 590rpx;
  padding: 3px 5px;
  box-sizing: border-box;
}

.goodsName {
  color: #333;
  font-size: 32rpx;
  height: 90rpx;
}

.desc {
  color: #999;
  font-size: 28rpx;
  height: 46rpx;
  line-height: 46rpx;
}

.price {
  display: flex;
  justify-content: space-between;
  height: 54rpx;
  line-height: 54rpx;
}

.price1 {
  color: #333;
  font-size: 32rpx;
}

.price2 {
  color: #666;
  font-size: 28rpx;
}

.ntegral {
  color: red;
  font-size: 32rpx;
}

.justSpace {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
